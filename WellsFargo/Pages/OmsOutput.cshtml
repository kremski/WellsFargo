@page
@model OmsOutputModel
@{
    ViewData["Title"] = "OMS output";
}

<div class="text-center">
    <h3>OMS output</h3>
    <br />
    <form method="post">
        <select name="OmsOutout" asp-items="Model.OmsOptions"></select>
        <input type="submit" />
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        @{
            if (TempData["success"] != null)
            {
                <p><b>@TempData["success"]</b></p>
            }
        }
    </form>
</div>